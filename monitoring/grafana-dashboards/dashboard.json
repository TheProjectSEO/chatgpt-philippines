{
  "dashboard": {
    "title": "ChatGPT Philippines - System Overview",
    "tags": ["chatgpt", "philippines", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{endpoint}} - {{method}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_errors_total[5m])",
            "legendFormat": "{{endpoint}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_ms_bucket[5m]))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "API Key Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "api_key_requests_total",
            "legendFormat": "Key: {{key}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(cache_operations_total{operation=\"hit\"}[5m])) / sum(rate(cache_operations_total{operation=~\"hit|miss\"}[5m]))",
            "legendFormat": "Hit Rate"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Queue Size",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(queue_size)",
            "legendFormat": "Total Jobs"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16}
      },
      {
        "id": 7,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(worker_count{status=\"busy\"})",
            "legendFormat": "Busy Workers"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Model Costs (USD)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(model_cost_usd_total[1h])",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}
